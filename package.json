{
    "name": "@uncover/js-swagger-generator",
    "version": "0.0.6",
    "author": "aSH <ash.uncover@gmail.com>",
    "description": "Javascript code generator based on swagger format",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ash-uncover/js-swagger-generator"
    },
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "type": "module",
    "bin": {
        "js-swagger-generator": "./bin/js-swagger-generator.js"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "scripts": {
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\" \"src/**/*.jsx\"",
        "prepare": "npm run build",
        "prepublishOnly": "npm run test",
        "test": "jest",
        "test-coverage": "jest --collectCoverage",
        "version": "npm run format"
    },
    "peerDependencies": {
        "@uncover/js-utils-fetch": "0.0.3"
    },
    "devDependencies": {
        "@types/jest": "29.5.4",
        "@types/node": "20.5.6",
        "jest": "29.6.4",
        "jest-junit": "16.0.0",
        "prettier": "3.0.2",
        "ts-jest": "29.1.1",
        "ts-loader": "9.4.4",
        "ts-node": "10.9.1",
        "typescript": "5.2.2"
    }
}
