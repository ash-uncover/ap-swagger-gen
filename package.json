{
    "name": "@uncover/js-swagger-generator",
    "version": "0.0.2",
    "author": "aSH <ash.uncover@gmail.com>",
    "description": "Javascript code generator based on swagger format",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/ash-uncover/js-swagger-generator"
    },
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "type": "module",
    "bin": {
        "js-swagger-generator": "./bin/js-swagger-generator.js"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "scripts": {
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\" \"src/**/*.jsx\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm run test && npm run lint",
        "preversion": "npm run lint",
        "test": "jest",
        "test-coverage": "jest --collectCoverage",
        "version": "npm run format"
    },
    "devDependencies": {
        "@types/jest": "latest",
        "@types/node": "latest",
        "jest": "latest",
        "jest-junit": "latest",
        "prettier": "latest",
        "ts-jest": "latest",
        "ts-loader": "latest",
        "ts-node": "latest",
        "tslint": "latest",
        "tslint-config-prettier": "latest",
        "typescript": "latest"
    }
}